{
  "canisters": {
    "dfinity_vue": {
      "main": "src/dfinity_vue/main.mo",
      "type": "motoko"
    },
    "dfinity_vue_assets": {
      "dependencies": [
        "dfinity_vue"
      ],
      "frontend": {
        "entrypoint": "src/dfinity_vue_assets/src/index.html"
      },
      "source": [
        "src/dfinity_vue_assets/assets",
        "dist/dfinity_vue_assets/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "dfx": "0.7.0",
  "networks": {
    "ic": {
      "providers": [
        "https://gw.dfinity.network"
      ],
      "type": "persistent"
    },
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}
